install.packages(c("curl", "devtools", "ggmap", "knitr", "mgcv", "nlme", "tidyr"))
data1 <- read.xlsx("data2013.xlsx", sheetName = "sheet1")
data2 <- geocode(as.character(data1$gu))
data3 <- cbind(data1, data2)
map.seoul <- qmap("seoul", source="stamen", zoom=12, maptype="toner", darken=c(.3, "#BBBBBB"))
map.seoul +
geom_point(data=data3, aes(x=lon, y=lat, size=price, color=occur), alpha=.9, na.rm=T) +
scale_color_gradient(low="#FF0000", high="#A90000", name="occur in 2013 ") +
scale_size(range=c(1,10) , guide="none") +
ggtitle("The Number & amount of fire damage in 2013 ") +
theme(text = element_text(family = "AppleGothic", color="#666666")) +
theme(plot.title = element_text( size=25, face="bold", hjust=0, vjust=.5))
